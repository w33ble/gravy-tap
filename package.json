{
  "name": "gravy-tap",
  "version": "0.1.0",
  "description": "Tap runner for Sauce Labs",
  "main": "index",
  "module": "index.mjs",
  "scripts": {
    "lint": "eslint \"*.{js,mjs}\" \"src/**/*.{js,mjs}\"",
    "test:output": "node -r esm test/parse_output.mjs",
    "test:runner": "rollup -c .rollup.test.js && node -r esm test/parse_test_output.mjs",
    "test": "npm run lint && npm run test:output && npm run test:runner",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "start": "node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w33ble/gravy-tap.git"
  },
  "keywords": [],
  "files": [
    "src/",
    "index.js",
    "index.mjs"
  ],
  "author": "Joe Fleming (https://github.com/w33ble)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/w33ble/gravy-tap/issues"
  },
  "homepage": "https://github.com/w33ble/gravy-tap",
  "lint-staged": {
    "*.{js,mjs,json,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "esm": {
    "cjs": true
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": ".changelog_template.hbs",
    "ignoreCommitPattern": "(chore|test):",
    "commitLimit": false
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "esm": "^3.0.17",
    "horsey-sauce": "^0.1.0",
    "simple-tap-parser": "^0.0.4"
  },
  "devDependencies": {
    "auto-changelog": "^1.4.6",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.9.0",
    "rollup": "^0.58.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "tape": "^4.9.0"
  }
}
